                errType = e.getTypeOfError();
                descr = e.getErrorDescription();
        } catch (JTSException e) {
                WFSUtil.printErr(engine,"", e);
                mainCode = WFSError.WF_OPERATION_FAILED;
                subCode = e.getErrorCode();
                subject = WFSErrorMsg.getMessage(mainCode);
