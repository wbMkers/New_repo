						descr = WFSErrorMsg.getMessage(subCode);
						errType = WFSError.WF_TMP;
					}
					if (rs != null) {
						rs.close();
						rs = null;
					}
					stmt.close();
					stmt = con.createStatement();
					if (mainCode == 0) {
						/* To check for modifiable rights on process. */
						StringBuffer tempStr = new StringBuffer(parser.toString());
