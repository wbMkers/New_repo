						hrs = rs.getInt(2);
						mins = rs.getInt(3);
					}
					pstmt.close();
					if (days >= 0) {
						timeInMins = days * 24 * 60;
					}
					if (hrs >= 0) {
						timeInMins = timeInMins + hrs * 60;
					}
					if (mins >= 0) {
