		sortStr = " DESC ";
	} else {
		sortStr = " ASC ";
	}
	String orderByStr=" ORDER BY TemplateName " + sortStr +" , TemplateId "+ sortStr ;

	try {
		engine = parser.getValueOf("EngineName");
