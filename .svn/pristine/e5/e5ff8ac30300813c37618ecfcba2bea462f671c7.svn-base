				pstmt = con.prepareStatement(query);
				WFSUtil.DB_SetString(1, procInstID, pstmt, dbType);
				parameters.add(procInstID);
				WFSUtil.jdbcExecute(procInstID, sessionID, userID, query, pstmt, parameters, debugFlag, engine);
