									//queryStr.append(WFSConstant.WF_VARCHARPREFIX + strConstantValue + "'");
									queryStr.append(WFSUtil.TO_STRING(strConstantValue, true, dbType));
								}
								queryStr.append(", lastModifiedOn = ");
