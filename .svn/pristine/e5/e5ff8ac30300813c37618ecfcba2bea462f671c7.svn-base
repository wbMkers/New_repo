				if(rs != null && rs.next()){
					procDefId = rs.getInt(1);
					rs.close();
					pstmt.close();
				}
				else {
					query = " Select ProcessDefId from QueueHistoryTable"+WFSUtil.getTableLockHintStr(dbType)+"where ProcessInstanceId = ? " + WFSUtil.getQueryLockHintStr(dbType);
