package com.newgen.omni.jts.util;

public class VariableClass {

	private int variableId;
	private String systemDefinedName;
	private String userDefinedName ;
	private String displayName ;

	public VariableClass(int variableId, String systemDefinedName,
			String userDefinedName, String displayName) {
		this.setVariableId(variableId);
		this.setSystemDefinedName(systemDefinedName);
		this.setUserDefinedName(userDefinedName);
		this.setDisplayName(displayName);
	}

	public void setDisplayName(String displayName) {
		this.displayName = displayName;
	}

	public String getDisplayName() {
		return displayName;
	}

	public void setUserDefinedName(String userDefinedName) {
		this.userDefinedName = userDefinedName;
	}

	public String getUserDefinedName() {
		return userDefinedName;
	}

	public void setSystemDefinedName(String systemDefinedName) {
		this.systemDefinedName = systemDefinedName;
	}

	public String getSystemDefinedName() {
		return systemDefinedName;
	}

	public void setVariableId(int variableId) {
		this.variableId = variableId;
	}

	public int getVariableId() {
		return variableId;
	}
}
